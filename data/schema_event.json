{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "SWVP Event Tick",
  "type": "object",
  "required": ["timestamp", "node_id", "domain_LRs", "f5_verified_fraction"],
  "properties": {
    "timestamp": { "type": "string", "description": "UTC ISO-8601" },
    "node_id": { "type": "string" },
    "domain_LRs": {
      "type": "object",
      "additionalProperties": { "type": "number" }
    },
    "f5_verified_fraction": { "type": "number", "minimum": 0.0, "maximum": 1.0 }
  }
}
